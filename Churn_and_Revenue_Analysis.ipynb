    "fig, ax = plt.subplots(1,4,figsize=(24,6))\n",
    "\n",
    "for i in range(4):\n",
    "    #Only take the 5 left-most bars from each of the above visualizations.\n",
    "    data = prop_df[i][:5]\n",
    "    sns.barplot(x='mission', y='proportion_users_remaining', ax=ax[i], data=data, color='blue')\n",
    "    ax[i].set(xlabel='', title='Player levels: {} to {}'.format(10*i + 2, 10*i + 11))\n",
    "    ax[i].title.set_size(20)\n",
